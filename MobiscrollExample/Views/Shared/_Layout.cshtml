    <!-- Driver Work Report Modal Start -->
    <div class="modal fade reports-general" id="driverWorkReport" tabindex="-1" data-bs-backdrop="static"
         aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title">@_localization.Getkey("WorkReport")</h3>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" id=" CloseDriverReport"></button>
                </div>
                <div class="modal-body">
                    <h4 class="modal-title" id="driverWorkReportModalLabel"> @_localization.Getkey("FiltersOfReport")</h4>
                    <div class="grid-wrapper">
                        <div class="grid-first-wrapper">
                            <div class="select-inputs ">
                                <span> @_localization.Getkey("DriverName")</span>
                                <select name="" id="driverList-work" class="full-width">
                                    <!-- Add dynamic options  -->
                                </select>
                            </div>
                        </div>
                        <div class="grid-sec-inputs-wrapper">
                            <div class="details-inputs">
                                <span>@_localization.Getkey("StartDate")</span>
                                <input type="date" id="driverFromDate" min="1000-01-01" max="9999-12-31">
                            </div>

                            <div class="details-inputs">
                                <span>@_localization.Getkey("EndDate")</span>
                                <input type="date" id="driverToDate" min="1000-01-01" max="9999-12-31">
                            </div>
                        </div>
                     
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-yellow" id="driverReport-Apply" onclick="generateDriverReport()">@_localization.Getkey("GenerateReport")</button>
                        <button type="button" class="btn btn-yellow" id="ResetModalDriverReport">@_localization.Getkey("Reset")</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Driver Work Report Modal End -->
    <!-- Weekend Report Modal -->
    <div class="modal fade reports-general" id="weekendReport" tabindex="-1" data-bs-backdrop="static"
         aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title">@_localization.Getkey("NightandSundayHoursReport")</h3>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" id="CloseWeekend" ></button>
                </div>
                <div class="modal-body">
                    <fieldset>
                        <h4 class="modal-title" id="weekendReportModalLabel"> @_localization.Getkey("FiltersOfReport")</h4>
                        <div class="grid-wrapper">
                            <div class="grid-first-wrapper">
                                <div class="select-inputs">
                                    <span> @_localization.Getkey("DriverName")</span>
                                    <select name="" id="driverList-sunday" class="full-width">
                                        <!-- Add dynamic options  -->
                                    </select>
                                </div>
                            </div>
                            <div class="grid-sec-inputs-wrapper">
                                <div class="details-inputs full-width">
                                    <span for="year"> @_localization.Getkey("Year")</span>
                                    <input type="number" id="yearOfWeekend" value="" placeholder=" @_localization.Getkey("EnterYear")" min="2000" max="2999" maxlength="4" onkeydown="javascript: return ['Backspace','Delete','ArrowLeft','ArrowRight'].includes(event.code) ? true : !isNaN(Number(event.key)) && event.code!=='Space'">
                                </div>
                            </div>

                        </div>
                    </fieldset>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-yellow" id="Generate-overTime" onclick="generateWeekendReport() ">
                        @_localization.Getkey("GenerateReport")
                    </button>
                    <button type="button" class="btn btn-yellow" id="ResetModalWeekend"> @_localization.Getkey("Reset")</button>
                </div>
            </div> 
        </div>
    </div>
    <!-- Weekend Report Modal End -->
