        <div class="vertical-alignment-helper">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-body">
                        <h4 id="alert-message"><span></span> </h4>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade alert-modal alert_message" id="alert-red" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
         aria-hidden="true" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="vertical-alignment-helper">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-body">
                        <h4 id="alert-message"><span></span> </h4>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Header End -->
    <!-- Popup Synchro Model Starts -->

    <div class="modal fade alert-modal alert_success" id="mnuAdminSyncPassengersForm" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
         aria-hidden="true" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="vertical-alignment-helper">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-body">
                        <div id="synchroSpinner" class="synchro-Spinner">
                            <div class="spinner-border text-muted text-center" id="loadingSpinner"></div>
                            <h6 class="text-center" id="loadingSpinText"> @_localization.Getkey("Running")...</h6>
                            <div class="text-center">
                                <p id="synchroMessageSuccess"></p>
                                <p id="synchroMessageError"></p>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-border" id="spninnerCloseBtn" disabled> @_localization.Getkey("closeBtn")</button>
                    </div>
                </div>
            </div>
