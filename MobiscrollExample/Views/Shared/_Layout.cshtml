                <div class="modal-header">
                    <h3 class="modal-title"> @_localization.Getkey("ReportDashboard")</h3>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" id="CloseModalDashboard"></button>
                </div>
                <div class="modal-body">
                    <h3 class="modal-title" id="journeyReportModalLabel"> @_localization.Getkey("FiltersOfReport")</h3>
                    <div class="grid-wrapper">
                        <div class="grid-sec-inputs-wrapper">
                            <div class="details-inputs">
                                <span>@_localization.Getkey("StartDate")</span>
                                <input type="date" id="startDate" name="startDate" min="1000-01-01" max="9999-12-31" required>
                            </div>

                            <div class="details-inputs">
                                <span>@_localization.Getkey("EndDate")</span>
                                <input type="date" id="endDate" name="endDate" min="1000-01-01" max="9999-12-31" required>
                            </div>
                            <div class="details-inputs btn-grp">
                                <button class="btn btn-yellow" id="ApplyId-dashboard" type="submit">@_localization.Getkey("Apply")</button>
                            </div>
                        </div>
                        <div class="grid-sec-inputs-wrapper">
                            <div class="details-inputs">
                                <label for="internalJourneys"> @_localization.Getkey("DashboardType1")</label>
                                <input type="text" id="internalJourneys" name="internalJourneys" disabled>
                            </div>
                            <div class="details-inputs">
                                <label for="externalJourneys"> @_localization.Getkey("DashboardType2")</label>
                                <input type="text" id="externalJourneys" name="externalJourneys" disabled>
                            </div>
                            <div class="details-inputs">
                                <label for="outOfPolicyJourneys"> @_localization.Getkey("DashboardType3")</label>
                                <input type="text" id="outOfPolicyJourneys" name="outOfPolicyJourneys" disabled>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-yellow" id="RefreshId">@_localization.Getkey("Refresh")</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Tableu de board Modal End -->
    

    <div class="modal fade alert-modal  alert_statement" id="confirm-popup" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
         aria-hidden="true" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="vertical-alignment-helper">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header"></div>
                    <div class="modal-body">
                        @{
                            string deleteQuestion = @_localization.Getkey("alertQuestionDelete");
                            string passenger = @_localization.Getkey("Passenger");
                            string deleteSatement = string.Format(deleteQuestion, passenger);
